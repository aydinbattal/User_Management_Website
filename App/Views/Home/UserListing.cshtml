@model IEnumerable<User>

@{
    int i = 1;
}

<h2>User Listing</h2>

@* <a asp asp-action="Dashboard" asp>Dashboard</a> *@
<a href="javascript:history.back()">Dashboard</a>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Phone Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <th scope="row">@i</th>
                <td>@user.FullName</td>
                <td>@user.Email</td>
                <td>@user.PhoneNumber</td>
            </tr>

            i++;
        }


    </tbody>
</table>